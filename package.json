{
  "name": "react-native-starter",
  "version": "0.0.1",
  "resolutions": {
    "deprecated-react-native-listview": "^0.0.9"
  },
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint:fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "flow": "./node_modules/flow-bin/cli.js",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.24.6",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-native-documents/picker": "^10.1.5",
    "@react-native-firebase/app": "^23.2.2",
    "@react-native-firebase/messaging": "^23.2.2",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native/new-app-screen": "0.80.0",
    "@react-navigation/bottom-tabs": "7.4.7",
    "@react-navigation/drawer": "7.5.8",
    "@react-navigation/native": "7.1.17",
    "@react-navigation/stack": "7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "Base64": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-circular-progress": "^1.4.1",
    "react-native-device-info": "^14.0.4",
    "react-native-fbsdk-next": "^13.4.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "8.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-maps": "^1.26.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-modal-dropdown": "1.0.2",
    "react-native-notification-sounds": "^0.5.8",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.4",
    "react-native-searchable-dropdown": "^1.1.3",
    "react-native-simple-radio-buttons": "^1.1.1",
    "react-native-simple-toast": "^3.3.2",
    "react-native-sticky-range-slider": "^0.1.7",
    "react-native-svg": "^15.12.1",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-icons": "^10.3.0",
    "react-native-version-check": "^3.5.0",
    "react-native-video": "6.16.1",
    "react-native-worklets": "^0.5.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "rn-double-click": "^1.0.3",
    "rn-fetch-blob": "^0.12.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.4.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "29.7.0",
    "danger": "12.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "flow-bin": "^0.237.2",
    "globals": "^15.3.0",
    "jest": "^29.6.3",
    "jsdom": "24.1.0",
    "jsdom-global": "3.0.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "plop": "^4.0.1",
    "prettier": "3.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^19.1.1",
    "react-test-renderer": "19.1.0",
    "sinon": "^18.0.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^7.12.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!android/**",
      "!ios/**",
      "!assets/**"
    ],
    "coverageDirectory": "jest_coverage",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone XS"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.5.3+sha512.3003a14012e2987072d244c720506549c1aab73ee728208f1b2580a9fd67b92d61ba6b08fe93f6dce68fd771e3af1e59a0afa28dd242dd0940d73b95fedd4e90"
}
