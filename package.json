{
  "name": "react-native-starter",
  "version": "0.0.1",
  "resolutions": {
    "deprecated-react-native-listview": "^0.0.9"
  },
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android",
    "lint:fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "flow": "./node_modules/flow-bin/cli.js",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.24.6",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/datetimepicker": "^8.1.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-native-google-signin/google-signin": "^12.1.1",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/drawer": "6.6.15",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/stack": "6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "Base64": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-native": "^0.73.8",
    "react-native-chart-kit": "^6.12.0",
    "react-native-circular-progress": "^1.4.0",
    "react-native-device-info": "^11.1.0",
    "react-native-document-picker": "9.3.0",
    "react-native-fbsdk-next": "^13.0.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-image-picker": "7.1.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-modal-dropdown": "1.0.2",
    "react-native-permissions": "3.9.0",
    "react-native-reanimated": "3.11.0",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-searchable-dropdown": "^1.1.3",
    "react-native-simple-radio-buttons": "^1.1.1",
    "react-native-simple-toast": "^3.3.1",
    "react-native-svg": "^15.3.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-ui-lib": "^7.23.5",
    "react-native-vector-icons": "^10.1.0",
    "react-native-version-check": "^3.4.7",
    "react-native-video": "5.2.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "rn-double-click": "^1.0.3",
    "rn-fetch-blob": "^0.12.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-flow": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@eslint/js": "^9.4.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "29.7.0",
    "danger": "12.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^9.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "flow-bin": "^0.237.2",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "jsdom": "24.1.0",
    "jsdom-global": "3.0.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "plop": "^4.0.1",
    "prettier": "3.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^18.3.1",
    "react-test-renderer": "18.3.1",
    "sinon": "^18.0.0",
    "typescript-eslint": "^7.12.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!android/**",
      "!ios/**",
      "!assets/**"
    ],
    "coverageDirectory": "jest_coverage",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone XS"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
